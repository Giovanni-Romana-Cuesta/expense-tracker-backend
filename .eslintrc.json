{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": "warn",
    "prettier/prettier": "error",
    "func-names": "off",
    "no-process-exit": "off",
    "no-useless-return": "off",
    "import/prefer-default-export": "off",
    "import/extensions": "off",
    "no-param-reassign": "off",
    "no-underscore-dangle": "off"
  }
}
